{
  "manifest_version": 3,
  "name": "Invensense",
  "version": "1.0.0",
  "description": "Invensense Dashboard - Your personalized new tab with dynamic wallpapers, cybersecurity news, bookmarks, and security tips",
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "Invensense Dashboard",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}