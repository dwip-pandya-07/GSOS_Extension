{
  "manifest_version": 3,
  "name": "GSOS",
  "version": "1.0.0",
  "description": "Security Awareness Dashboard",
  "permissions": ["storage"],
  "host_permissions": ["http://127.0.0.1:8000/*"],

  "action": {
    "default_title": "GSOS - Security Dashboard",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },

  "background": {
    "service_worker": "background.js"
  },

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
